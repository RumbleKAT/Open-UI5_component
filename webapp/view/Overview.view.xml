 <mvc:View controllerName="com.myorg.myUI5App.controller.Overview"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	 <Shell>
        <App class="myAppDemoWT" id="app">
            <pages>
                <Page title="{i18n>homePageTitle}">
                    <headerContent>
                        <Button icon="sap-icon://hello-world" press=".onOpenDialog"/>
                    </headerContent>
                    <content>
                        <mvc:XMLView viewName="com.myorg.myUI5App.view.HelloPanel"/>
                        <mvc:XMLView viewName="com.myorg.myUI5App.view.InvoiceList"/>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View> 
